# Dolphin Storage Server - Environment Configuration
# Copy this file to .env and set your values

# API Key for authentication (REQUIRED - change this!)
STORAGE_API_KEY=your-secure-api-key-here

# Storage configuration
STORAGE_PATH=/data/storage
DATABASE_PATH=/data/storage.db

# Server settings
PORT=5000
DEBUG=false

# Auto-delete files after download (true/false)
AUTO_DELETE=true
