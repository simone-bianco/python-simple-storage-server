{
  "info": {
    "_postman_id": "dolphin-storage-server-collection",
    "name": "Dolphin Storage Server API",
    "description": "Collection for testing the Dolphin Storage Server API locally and in production. This lightweight Flask server handles ZIP file uploads from RunPod workers, with optional auto-delete after download.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "key",
        "value": "X-API-Key",
        "type": "string"
      },
      {
        "key": "value",
        "value": "{{storage_api_key}}",
        "type": "string"
      },
      {
        "key": "in",
        "value": "header",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "auth": {
          "type": "noauth"
        },
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        },
        "description": "Health check endpoint. Returns server status, service name, and current timestamp. No authentication required."
      },
      "response": [
        {
          "name": "Health Check - Success",
          "originalRequest": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/health",
              "host": ["{{base_url}}"],
              "path": ["health"]
            }
          },
          "status": "OK",
          "code": 200,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"status\": \"ok\",\n  \"service\": \"dolphin-storage-server\",\n  \"timestamp\": \"2024-01-18T15:30:00.123456\"\n}"
        }
      ]
    },
    {
      "name": "Upload File (Multipart)",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": "",
              "description": "ZIP file to upload"
            },
            {
              "key": "job_id",
              "value": "{{job_id}}",
              "type": "text",
              "description": "Unique job identifier"
            }
          ]
        },
        "url": {
          "raw": "{{base_url}}/upload",
          "host": ["{{base_url}}"],
          "path": ["upload"]
        },
        "description": "Upload a ZIP file using multipart/form-data. Requires 'file' field with the ZIP and 'job_id' field with a unique identifier."
      },
      "response": [
        {
          "name": "Upload - Success",
          "originalRequest": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{base_url}}/upload",
              "host": ["{{base_url}}"],
              "path": ["upload"]
            }
          },
          "status": "Created",
          "code": 201,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"status\": \"uploaded\",\n  \"job_id\": \"job-123\",\n  \"file_size\": 12345,\n  \"download_url\": \"/download/job-123\"\n}"
        },
        {
          "name": "Upload - Missing Job ID",
          "originalRequest": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{base_url}}/upload",
              "host": ["{{base_url}}"],
              "path": ["upload"]
            }
          },
          "status": "Bad Request",
          "code": 400,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"error\": \"Missing job_id\"\n}"
        }
      ]
    },
    {
      "name": "Upload File (Raw Binary)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "X-Job-Id",
            "value": "{{job_id}}",
            "type": "text",
            "description": "Job ID passed as header for raw binary uploads"
          },
          {
            "key": "Content-Type",
            "value": "application/octet-stream",
            "type": "text"
          }
        ],
        "body": {
          "mode": "file",
          "file": {
            "src": ""
          }
        },
        "url": {
          "raw": "{{base_url}}/upload",
          "host": ["{{base_url}}"],
          "path": ["upload"]
        },
        "description": "Upload a ZIP file as raw binary data. Job ID must be provided in the X-Job-Id header."
      },
      "response": []
    },
    {
      "name": "Download File",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/download/{{job_id}}",
          "host": ["{{base_url}}"],
          "path": ["download", "{{job_id}}"]
        },
        "description": "Download a ZIP file by job_id. By default, the file is automatically deleted after download if AUTO_DELETE is enabled on the server."
      },
      "response": [
        {
          "name": "Download - Success",
          "originalRequest": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/download/{{job_id}}",
              "host": ["{{base_url}}"],
              "path": ["download", "{{job_id}}"]
            }
          },
          "status": "OK",
          "code": 200,
          "_postman_previewlanguage": "text",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/zip"
            },
            {
              "key": "Content-Disposition",
              "value": "attachment; filename=job-123.zip"
            }
          ],
          "body": "<binary ZIP data>"
        },
        {
          "name": "Download - Not Found",
          "originalRequest": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/download/non-existent-job",
              "host": ["{{base_url}}"],
              "path": ["download", "non-existent-job"]
            }
          },
          "status": "Not Found",
          "code": 404,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"error\": \"Job not found\"\n}"
        },
        {
          "name": "Download - Already Deleted",
          "originalRequest": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/download/deleted-job",
              "host": ["{{base_url}}"],
              "path": ["download", "deleted-job"]
            }
          },
          "status": "Gone",
          "code": 410,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"error\": \"File already deleted\"\n}"
        }
      ]
    },
    {
      "name": "Download File (Keep)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/download/{{job_id}}?keep=true",
          "host": ["{{base_url}}"],
          "path": ["download", "{{job_id}}"],
          "query": [
            {
              "key": "keep",
              "value": "true",
              "description": "Prevents auto-delete after download"
            }
          ]
        },
        "description": "Download a ZIP file by job_id without triggering auto-delete. Set keep=true to preserve the file for multiple downloads."
      },
      "response": []
    },
    {
      "name": "Delete File",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": {
          "raw": "{{base_url}}/delete/{{job_id}}",
          "host": ["{{base_url}}"],
          "path": ["delete", "{{job_id}}"]
        },
        "description": "Manually delete a stored file by job_id. The file is removed from disk and marked as deleted in the database."
      },
      "response": [
        {
          "name": "Delete - Success",
          "originalRequest": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "{{base_url}}/delete/{{job_id}}",
              "host": ["{{base_url}}"],
              "path": ["delete", "{{job_id}}"]
            }
          },
          "status": "OK",
          "code": 200,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"status\": \"deleted\",\n  \"job_id\": \"job-123\"\n}"
        },
        {
          "name": "Delete - Not Found",
          "originalRequest": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "{{base_url}}/delete/non-existent-job",
              "host": ["{{base_url}}"],
              "path": ["delete", "non-existent-job"]
            }
          },
          "status": "Not Found",
          "code": 404,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"error\": \"Job not found or already deleted\"\n}"
        }
      ]
    },
    {
      "name": "List Files",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/list",
          "host": ["{{base_url}}"],
          "path": ["list"]
        },
        "description": "List all stored files for administrative purposes. Returns the last 100 files with their metadata including job_id, file_size, upload time, download time, and deletion status."
      },
      "response": [
        {
          "name": "List - Success",
          "originalRequest": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/list",
              "host": ["{{base_url}}"],
              "path": ["list"]
            }
          },
          "status": "OK",
          "code": 200,
          "_postman_previewlanguage": "json",
          "body": "{\n  \"files\": [\n    {\n      \"job_id\": \"job-123\",\n      \"file_size\": 12345,\n      \"uploaded_at\": \"2024-01-18T15:30:00\",\n      \"downloaded_at\": null,\n      \"deleted\": false\n    },\n    {\n      \"job_id\": \"job-456\",\n      \"file_size\": 67890,\n      \"uploaded_at\": \"2024-01-18T14:00:00\",\n      \"downloaded_at\": \"2024-01-18T14:30:00\",\n      \"deleted\": true\n    }\n  ],\n  \"count\": 2\n}"
        }
      ]
    }
  ],
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [""]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [""]
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:5000",
      "type": "string",
      "description": "Base URL of the Dolphin Storage Server"
    },
    {
      "key": "storage_api_key",
      "value": "your-api-key-here",
      "type": "string",
      "description": "API Key for authentication"
    },
    {
      "key": "job_id",
      "value": "test-job-123",
      "type": "string",
      "description": "Job ID for upload/download/delete operations"
    }
  ]
}
